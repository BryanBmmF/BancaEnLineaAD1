<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ingreso.RedireccionInicialActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="118dp"
        android:layout_height="70dp"
        android:text="Iniciando Aplicacion"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="91dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="187dp"
        android:layout_height="133dp"
        android:text="Se ha detectado que es la primera vez que inicias la aplicacion. Por favor dinos si quieres agregar la funcionalidad de poder iniciar la aplicacion con tu huella digital del telefono"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="223dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="277dp"
        android:layout_height="55dp"
        android:text="Si, quiero iniciar con huella"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="366dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="247dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:text="No gracias, prefiero continuar sin huella"
        app:layout_constraintStart_toStartOf="@+id/button"
        tools:layout_editor_absoluteY="429dp" />
</androidx.constraintlayout.widget.ConstraintLayout>